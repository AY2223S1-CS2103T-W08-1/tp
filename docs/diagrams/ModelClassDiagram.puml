@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Class "<<interface>>\nReadOnlyHealthContact" as ReadOnlyHealthContact
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class HealthContact
Class ModelManager
Class UserPrefs

Class UniquePatientList
Class Patient
Class Address
Class Email
Class Name
Class Phone
Class Tag

Class UniqueAppointmentList
Class Appointment
Class Name
Class Slot
Class Doctor
Class MedicalTest

Class UniqueBillList
Class Bill
Class Appointment
Class Amount
Class BillDate
Class PaymentStatus
}

Class HiddenOutside #FFFFFF
HiddenOutside .> Model


ModelManager .up.|> Model

HealthContact .up.|> ReadOnlyHealthContact

Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyHealthContact

ModelManager -left-> "1" HealthContact
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs



HealthContact *-----> "1" UniquePatientList
UniquePatientList --> "~* all" Patient
Patient *--> Name
Patient *--> Phone
Patient *--> Email
Patient *--> Address
Patient *--> "*" Tag


HealthContact *---> "1" UniqueAppointmentList
UniqueAppointmentList --> "~* all" Appointment
Appointment *--> Name
Appointment *--> Slot
Appointment *--> Doctor
Appointment *--> MedicalTest



HealthContact *----> "1" UniqueBillList
UniqueBillList --> "~* all" Bill
Bill *--> Appointment
Bill *--> Amount
Bill *--> BillDate
Bill *--> PaymentStatus


Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email


ModelManager -->"~* filtered" Appointment
ModelManager -->"~* filtered" Bill
ModelManager -->"~* filtered" Patient
@enduml
